<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<layout>
  <display-name>Focus boxes</display-name>
  <config>
    <field-set name="basic">
      <label>Konfigurasjon</label>
      <items>
        <input name="noPadding" type="Checkbox">
          <label>No top and bottom padding</label>
          <occurrences minimum="0" maximum="0"/>
        </input>
      </items>
    </field-set>
  </config>
  <regions>
    <region name="left"/>
    <region name="center"/>
    <region name="right"/>
  </regions>
</layout>